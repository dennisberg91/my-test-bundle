{
    "name": "dennisberg91/my-test-bundle",
    "description": "Test bundle to practice making a Symfony bundle.",
    "type": "symfony-bundle",
    "version": "1.0",
    "authors": [
        {
            "name": "Dennis van den Berg",
            "email": "dennis91@live.nl"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "symfony/http-kernel": "^5.1",
        "symfony/http-client": "^5.1"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "MichielBakker\\MyTestBundle\\": "src"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "5.x-dev"
    },
    "scripts": {
        "test": "vendor/bin/simple-phpunit"
    }
}
